    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-perfectionist": "2.5.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-solid": "0.13.1",
    "eslint": "^8.56.0",
    "prettier-plugin-tailwindcss": "^0.5.12",
