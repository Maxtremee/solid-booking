    "@solidjs/start": "0.7.5",
    "arctic": "1.2.1",
    "autoprefixer": "10.4.18",
    "drizzle-orm": "0.30.2",
    "lucia": "3.1.1",
    "oslo": "1.1.3",
    "postcss": "8.4.35",
    "solid-icons": "^1.1.0",
    "solid-js": "1.8.15",
    "tailwind-variants": "0.2.0",
